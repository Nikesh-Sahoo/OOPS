#include <iostream>
#include<queue>
using namespace std;
int main()
{
    queue<string>q;
    q.push("NIKESH");
    q.push("SAHOO");
    q.push("NIKU");
    q.push("SAHOO");
    cout<<"Front Element: "<<q.front()<<endl;
    cout<<"size of queue: "<<q.size()<<endl;
    cout<<"Before pop: "<<endl;
    cout<<"size of queue: "<<q.size()<<endl;
    q.pop();
    cout<<"After pop: "<<endl;
    cout<<"size of queue: "<<q.size()<<endl;
    cout<<"Empty or not: "<<q.empty()<<endl;
    return 0;
}
