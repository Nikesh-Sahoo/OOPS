#include <iostream>
#include<stack>
using namespace std;
int main()
{
    stack<string>s;
    s.push("NIKESH");
    s.push("SAHOO");
    s.push("NIKU");
    s.push("SAHOO");
    cout<<"size of stack: "<<s.size()<<endl;
    cout<<"Element at the top: "<<s.top()<<endl;
    cout<<"Before pop: "<<endl;
    cout<<"size of stack: "<<s.size()<<endl;
    s.pop();
    cout<<"After pop: "<<endl;
    cout<<"size of stack: "<<s.size()<<endl;
    cout<<"Element at the top after pop operation: "<<s.top()<<endl;
    cout<<"Empty or not: "<<s.empty()<<endl;
    return 0;
}
