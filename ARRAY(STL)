#include <iostream>
#include<array>
using namespace std;
int main()
{
    array<int,5>a;
    for(int i=0;i<a.size();i++)
    {
        cin>>a[i];
    }
    for(int i=0;i<a.size();i++)
    {
        cout<<a[i]<<" ";
    }
    
    cout<<endl;
    cout<<"At first index->"<<a.front()<<endl;
    cout<<"At last index->"<<a.back()<<endl;
    cout<<"At index 2 value is-> "<<a.at(2)<<endl;
    cout<<"Empty or not->"<<a.empty()<<endl;
    return 0;
}
