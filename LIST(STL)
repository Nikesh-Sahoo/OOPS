#include <iostream>
#include<list>
using namespace std;
int main()
{
    list<int>l;
    l.push_back(1);
    l.push_back(2);
    l.push_back(3);
    l.push_back(4);
    l.push_front(5);
    cout<<"Before erase: "<<endl;
    cout<<"Size of array: "<<l.size()<<endl;
    l.erase(l.begin());
    cout<<"After erase: "<<endl;
    cout<<"Size of array->"<<l.size()<<endl;
    cout<<"At first index->"<<l.front()<<endl;
    cout<<"At last index->"<<l.back()<<endl;
    cout<<"Before pop_back: "<<endl;
    cout<<l.back()<<endl;
    cout<<"After pop_back: "<<endl;
    l.pop_back();
    cout<<l.back()<<endl;
    cout<<"Empty or not->"<<l.empty()<<endl;
    return 0;
}
